package selfNumber;

public class SelfNumber {
	
	
	private int selfNumber(){
		
		long result = 0;
		
		for(int i = 0; i < 5000; i++){
			if(isSelfNumber(i))
				result += i;
		}
		
		return result;
	}
	
	private boolean isSelfNumber(int n){
		
		return true;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		SelfNumber sn = new SelfNumber();
		
		System.out.println(sn.selfNumber());
	}

}
