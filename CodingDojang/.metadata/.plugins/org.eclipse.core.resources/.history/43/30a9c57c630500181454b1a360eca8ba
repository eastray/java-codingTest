package multiplesOf3And5;

import java.util.stream.IntStream;

public class MultiplesOf3And5 {

	private int multiple(int n, int m) {
		int result = 0;

		for (int i = 1; i < 10; i++) 
			if (i % n == 0 || i % m == 0)
				result += i;

		return result;
		
//		return IntStream.of(1000).map(ele ->{
//			if(ele%n ==0 || ele %m ==0)
//			 return ele;
//			return 0;
//		}).mapToLong(ele -> ele).sum();

	}

	public static void main(String[] args) {
		MultiplesOf3And5 moa = new MultiplesOf3And5();

		System.out.println(moa.multiple(3, 5));

	}

}
