package caesar;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class Caesar {

	private String caesar(String s, int n) {
		
		char[] stringArray = s.replaceAll("\\s", "").toCharArray();

		List<char[]> stringList = Arrays.asList(stringArray);
		
		System.out.println(stringList);
		
//		List<Integer> intList = stringList.stream().map(ele -> Integer.parseInt(ele.toString()) + n).collect(Collectors.toList());
//		System.out.println(intList);
		
		
//		List<Integer> intList = stringList.stream().filter(ele -> !ele.isEmpty()).map(ele -> Integer.parseInt(s) + n)
//				.collect(Collectors.toList());
//		System.out.println(intList);

		String temp = "123";
		System.out.println(Integer.parseInt(temp) + 3);

		String result = "";
		// 함수를 완성하세요.

		return result;
	}

	public static void main(String[] args) {
		Caesar c = new Caesar();
		System.out.println("s는 'a B z', n은 4인 경우: " + c.caesar("a B z", 4));
	}

}
