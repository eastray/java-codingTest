package removeSmallNumber;

import java.util.Arrays;

public class RemoveSmallNumber {
	
	private int[] removeSmallNumber (int[] array ){
		
		int min = 0;
		
		System.out.println(Arrays.stream(array).min() == 1 ? true : false);
		
		return null;
	}

