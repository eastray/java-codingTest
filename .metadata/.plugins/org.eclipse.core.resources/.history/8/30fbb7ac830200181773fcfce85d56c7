package eatCookie;

public class EatCookie {
	public int eatCookie(int[] cookies) {

		int answer = 0;
		int temp = cookies[0];

		for (int ele : cookies) {

			if (temp < ele)
				answer++;

		}

		return answer;
	}

	public static void main(String[] args) {

		EatCookie e = new EatCookie();

		int[] cookies = { 1, 4, 2, 6, 3, 4, 1, 5 };

		System.out.println(e.eatCookie(cookies));
	}

}
