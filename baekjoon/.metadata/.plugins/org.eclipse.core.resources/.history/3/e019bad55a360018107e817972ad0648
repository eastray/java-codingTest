package distributedComputing;

import java.math.BigInteger;
import java.util.Scanner;

public class DistributedComputing {

	public int distributed(int n, int m) {

		BigInteger amount = BigInteger.valueOf((long) Math.pow(n, m));

		System.out.println(amount);
		
		for(int i = 1; i < 15; i ++){
			System.out.println((long)Math.pow(3, i));
		}

		return 0;
	} 
	//3: 
	//4: 4, 6
	//5: 5
	//6: 6
	//7: 7, 9, 3, 1 
	//8: 8, 4, 2, 6
	//9: 9, 1

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		DistributedComputing dc = new DistributedComputing();

		Scanner sc = new Scanner(System.in);

		int n = sc.nextInt();

		for (int i = 0; i < n; i++) {
			dc.distributed(sc.nextInt(), sc.nextInt());
			System.out.println();
			
		}

	}

}
