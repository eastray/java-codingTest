package problem2;

public class SumOfFibonacciNumbers {

	private int sumOfFibonacci(int n, int s, int e) {
		if (e > n)
			return 0;

		return s + sumOfFibonacci(n, e, s + e);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		SumOfFibonacciNumbers sofn = new SumOfFibonacciNumbers();

		System.out.println(sofn.sumOfFibonacci(10, 1, 2));
	}

}
