package problem20;

public class SumOfFactorial {
	
	private int factorial(int n){
		int sum = 1;
		
		for(int i = 0; i < n; i++)
			sum *= (n - i);
		
		return sum;
	}
	
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		SumOfFactorial sof = new SumOfFactorial();
		
		System.out.println(sof.factorial(10));
	}

}
