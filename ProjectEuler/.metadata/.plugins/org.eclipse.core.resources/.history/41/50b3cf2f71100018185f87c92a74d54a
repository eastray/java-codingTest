package problem10;

import java.util.ArrayList;
import java.util.Arrays;

public class PrimeNumber {

	private ArrayList<Integer> getPrimeNumber(int n) {

		ArrayList<Integer> list = new ArrayList<Integer>();

		for (int i = 2; i < n; i++) {

			boolean flag = true;
			int temp = i % 10;

			if (i == 2 || i == 5 || temp == 1 || temp == 3 || temp == 7 || temp == 9) {
				for (int j = 2; j < i; j++) {
					if (i % j == 0) {
						flag = false;
						break;
					}
				}

				if (flag)
					list.add(i);

			}

		}

		// return list.stream().mapToInt(m->m).toArray();
		return list;
	}

	private int getPrimeNumber2(int n) {
		boolean[] arr = new boolean[n + 1];

		for (int i = 2; i <= n; i++)
			arr[i] = true;

		// 2 부터 ~ i*i <= n
		// 각각의 배수들을 지워간다.
		for (int i = 2; (i * i) <= n; i++) {
			if (primeList.get(i)) {
				for (int j = i * i; j <= n; j += i)
					primeList.set(j, false);
			}
		}

		return 0;
	}

	private int sumOfPrimeNumber(int n) {

		System.out.println(getPrimeNumber2(n));

		return 0;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		PrimeNumber pn = new PrimeNumber();

		System.out.println(pn.sumOfPrimeNumber(10));

	}

}
