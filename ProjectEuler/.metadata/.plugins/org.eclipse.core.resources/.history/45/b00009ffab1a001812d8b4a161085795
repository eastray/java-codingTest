package problem25;

public class NthOfFibonacci {

	private int fibo2(int n) {
		if (n <= 2)
			return 1;

		return fibo2(n - 1) + fibo2(n - 2);
	}

	private long fibo(long n) {

		if (n <= 2)
			return 1;
		
		long temp = 1;
		long result = 1;
		long last = 1;

		for (int i = 3; i <= n; i++) {
			temp = result;
			result += last;
			last = temp;
		}

		return result;
	}
	
	private int nthOfFibonacci(int n){
		int result = 0 ;
		//int length = Integer.toString(n).length();
		int index = 10;

		while(true){
			
			String temp = Integer.toString(fibo(index));
			System.out.println(temp);
			
			if(temp.length() >= n){
				result = index;
				break;
			}
			
			index++;
				
			
		}
		
		return result;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		NthOfFibonacci nof = new NthOfFibonacci();

		System.out.println(nof.nthOfFibonacci(100));
	}

}
