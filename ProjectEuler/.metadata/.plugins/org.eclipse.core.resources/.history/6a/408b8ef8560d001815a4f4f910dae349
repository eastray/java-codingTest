package problem2;

import java.util.stream.IntStream;

public class SumOfFibonacciNumbersUsingStream {
	
	private int sumOfFibonacci(int n) {
		return IntStream.rangeClosed(1, n).reduce((a, b) -> a + b).getAsInt();
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		SumOfFibonacciNumbersUsingStream sofnus = new SumOfFibonacciNumbersUsingStream();
		
		System.out.println(sofnus.sumOfFibonacci(10));
	}

}
