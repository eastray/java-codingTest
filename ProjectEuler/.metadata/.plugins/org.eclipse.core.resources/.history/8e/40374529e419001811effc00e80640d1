package problem20;

public class SiumOfMultiplyFactorial {
	
	private long factorial(long n){
		long sum = 1;
		
		for(int i = 0; i < n; i++){
			sum *= (n - i);
			System.out.println(sum);
		}
		
		return sum;
	}
	
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		SiumOfMultiplyFactorial sof = new SiumOfMultiplyFactorial();
		
		System.out.println(sof.factorial(100));
	}

}
