package problem10;

import java.util.ArrayList;
import java.util.Arrays;

public class PrimeNumber {
	private int[] getPrimeNumber(int n) {
		ArrayList<Integer> list = new ArrayList<Integer>();

		for (int i = 2; i <= n; i++) {

			boolean flag = true;

			int temp = i % 10;

			if (i < 10) {

				for (int j = 2; j < i; j++)
					if (i % j == 0) {
						flag = false;
						break;
					}

				if (flag)
					list.add(i);
			}

			else if (temp == 1 || temp == 3 || temp == 7 || temp == 9) {

				for (int j = 2; j < i; j++)
					if (i % j == 0) {
						flag = false;
						break;
					}

				if (flag)
					list.add(i);

				System.out.println("i: " + i);
			}

		}

		return list.stream().mapToInt(m -> m).toArray();
	}

	private ArrayList<Integer> getPrimeNumber2(int n) {

		ArrayList<Integer> list = new ArrayList<Integer>();

		for (int i = 2; i < n; i++) {
			boolean flag = true;

			for (int j = 3; j < i; j++) {
				if (i % j == 0) {
					flag = false;
					break;
				}
			}

			if (flag)
				list.add(i);
		}

		return list;
	}

	private int sumOfPrimeNumber(int n) {

		// System.out.println(Arrays.toString(getPrimeNumber(n)));

		getPrimeNumber2(n).stream().forEach(System.out::println);

		// System.out.println(getPrimeNumber2(n).stream().mapToInt(m ->
		// m).sum());

		return 0;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		PrimeNumber pn = new PrimeNumber();

		System.out.println(pn.sumOfPrimeNumber(10));
		//
		// System.out.println(pn.sumOfPrimeNumber(1000000));

		// System.out.println(pn.sumOfPrimeNumber(2000000));

	}

}
